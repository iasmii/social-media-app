<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.lab7bun.LogInController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="logInButton" layoutX="227.0" layoutY="267.0" mnemonicParsing="false"
                onAction="#onLogInButtonCLick" prefHeight="26.0" prefWidth="111.0" text="Log In" textAlignment="CENTER"
                textFill="#1408ff" AnchorPane.leftAnchor="225.0" AnchorPane.rightAnchor="225.0"/>
        <TextField fx:id="nume" layoutX="227.0" layoutY="140.0" prefHeight="26.0" prefWidth="150.0" promptText="Nume" AnchorPane.leftAnchor="225.0" AnchorPane.rightAnchor="225.0" />
        <TextField fx:id="prenume" layoutX="227.0" layoutY="180.0" prefHeight="26.0" prefWidth="150.0" promptText="Prenume" AnchorPane.leftAnchor="225.0" AnchorPane.rightAnchor="225.0" />
        <PasswordField fx:id="password" layoutX="227.0" layoutY="220.0" prefHeight="26.0" prefWidth="150.0" promptText="Password" AnchorPane.leftAnchor="225.0" AnchorPane.rightAnchor="225.0" />
        <Hyperlink layoutX="326.0" layoutY="294.0" onAction="#onSignInClick" prefHeight="18.0" prefWidth="42.0"
                   text="Sign up!" underline="true" wrapText="true" AnchorPane.leftAnchor="326.0"
                   AnchorPane.rightAnchor="232.0"/>
        <Label layoutX="237.0" layoutY="289.0" prefHeight="27.0" prefWidth="91.0" text="Nu ai cont?"
               AnchorPane.leftAnchor="237.0" AnchorPane.rightAnchor="272.0"/>
        <Label layoutX="233.0" layoutY="82.0" text="Log in" textFill="#0015f7" AnchorPane.leftAnchor="233.0" AnchorPane.rightAnchor="217.0"/>
    </children>
</AnchorPane>
